package loops;
import java.util.Scanner;
public class fibbo {

	public static void main(String[] args) {
		int n1=0,n2=1,n3;
		Scanner obj =new Scanner(System.in);
		System.out.println("Enter the range that you want to find fibonacci series");
		int num=obj.nextInt();
		System.out.print(n1+" "+n2);
		for(int i=2;i<num;i++) {
			n3=n1+n2;
			System.out.println(""+n3);
			n1=n2;
			n2=n3;
		}
		obj.close();
	}

}
